{
    "manifest_version": 3,
    "name": "Web Content Extractor",
    "version": "1.0",
    "description": "Extract web content from the currently active tab",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
      }
    },
    "icons": {
      "128": "images/icon128.png"
    }
  }
  